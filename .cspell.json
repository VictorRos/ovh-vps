{
	"version": "0.2",
	"language": "en-GB,en-US,fr-FR",
	"import": [
		"@cspell/dict-bash/cspell-ext.json",
		"@cspell/dict-companies/cspell-ext.json",
		"@cspell/dict-css/cspell-ext.json",
		"@cspell/dict-docker/cspell-ext.json",
		"@cspell/dict-fr-fr/cspell-ext.json",
		"@cspell/dict-git/cspell-ext.json",
		"@cspell/dict-html/cspell-ext.json",
		"@cspell/dict-k8s/cspell-ext.json",
		"@cspell/dict-markdown/cspell-ext.json",
		"@cspell/dict-node/cspell-ext.json",
		"@cspell/dict-npm/cspell-ext.json",
		"@cspell/dict-powershell/cspell-ext.json",
		"@cspell/dict-python/cspell-ext.json",
		"@cspell/dict-software-terms/cspell-ext.json",
		"@cspell/dict-sql/cspell-ext.json",
		"@cspell/dict-typescript/cspell-ext.json"
	],
	"caseSensitive": false,
	"languageSettings": [
		{
			"languageId": "*",
			"locale": "fr-FR",
			"caseSensitive": false
		}
	],
	"ignorePaths": [
		"**/.git/**",
		"**/.vscode/**",
		"**/build/**",
		"**/dist/**",
		"**/megalinter-reports/**",
		"**/node_modules/**",
		"**/vscode-extension/**",
		"**/ansible/files/**",
		"**/ansible/templates/**",
		"**/test/**",
		"**/package-lock.json",
		"**/*.feature",
		"**/*.spec.{js,jsx,ts,tsx}",
		"**/*.{ico,jpeg,jpg,svg,png}",
		".*"
	],
	"ignoreWords": [],
	"ignoreRegExpList": ["data-image-base64", "email", "hostnames", "jwt-token", "uuid", "uuid-non-hexadecimal"],
	"patterns": [
		{
			"name": "data-image-base64",
			"description": "Base64 encoded images",
			"pattern": "/data:image/[^;]+;base64,.+/g"
		},
		{
			"name": "email",
			"description": "Email addresses",
			"pattern": "/[\\w\\._-]+@[\\w\\._-]+\\.[a-z]{2,4}/g"
		},
		{
			"name": "hostnames",
			"description": "Hostnames",
			"pattern": ["/([^.]+\\.)?azurecr\\.io/g", "/([^.]+\\.)?onmicrosoft\\.com/g"]
		},
		{
			"name": "jwt-token",
			"description": "JWT token",
			"pattern": "/eyJ[A-Za-z0-9-_]+\\.[A-Za-z0-9-_]+\\.[A-Za-z0-9-_]+/g"
		},
		{
			"name": "uuid",
			"description": "UUID",
			"pattern": "/[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}/g"
		},
		{
			"name": "uuid-non-hexadecimal",
			"description": "UUID with non-hexadecimal characters",
			"pattern": "/[0-9a-zA-Z]{8}-[0-9a-zA-Z]{4}-[0-9a-zA-Z]{4}-[0-9a-zA-Z]{4}-[0-9a-zA-Z]{12}/g"
		}
	],
	"words": [
		"DATAPATH",
		"NOPASSWD",
		"VSPATH",
		"Vintagestory",
		"autoclean",
		"autoremove",
		"bantime",
		"blockinfile",
		"dearmor",
		"findtime",
		"libbz",
		"libc",
		"libgdbm",
		"libncursesw",
		"libnss",
		"libopenal",
		"libreadline",
		"libsqlite",
		"lineinfile",
		"maxretry",
		"nofile",
		"pyenv",
		"runtimeconfig",
		"serverconfig",
		"servermagicnumbers",
		"vintagestory"
	]
}
